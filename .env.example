# Django
DEBUG=1
SECRET_KEY=change-me
DJANGO_ALLOWED_HOSTS=localhost 127.0.0.1 [::1]

# Database (PostgreSQL)
DB_ENGINE=django.db.backends.postgresql
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=db
DB_PORT=5432

# Авто-миграции и загрузка метрик при старте контейнера
AUTO_INGEST=1
# Файлы по умолчанию (лежат рядом с manage.py). При наличии данных в БД ingest пропускается.
INGEST_YEAR_1=2022
INGEST_VERSION_1=v1.0 (2022)
INGEST_VISITS_1=2022_yandex_metrika_visits.parquet
INGEST_HITS_1=2022_yandex_metrika_hits.parquet
INGEST_YEAR_2=2024
INGEST_VERSION_2=v2.0 (2024)
INGEST_VISITS_2=2024_yandex_metrika_visits.parquet
INGEST_HITS_2=2024_yandex_metrika_hits.parquet

# Yandex AI (для генерации гипотез)
folder_id=your-folder-id
api_key=your-api-key

INGEST_SAMPLE_LIMIT=42634